      "source": [
        "import pandas as pd\n",
        "\n",
        "def order_scores(scores: pd.DataFrame) -> pd.DataFrame:\n",
        "    scores['rank'] = scores.sort_values(by = 'score', ascending = False)['score'].rank(ascending = False, method = 'dense')\n",
        "    return scores.sort_values(by = 'rank')[['score', 'rank']]"
      ]
    }
  ]
